    </partitioning>
    <connection>FI-Admin</connection>
    <sql>select 	'lilacs' as FI, 
		case when rf.id = an.reference_ptr_id and rf.id = al.reference_id then  
				concat('biblio-',convert(rf.id, char))  
			when an.source_id = so.reference_ptr_id then 
				concat('lil-',convert(an.reference_ptr_id, char)) 
			else 
				rf.id 
		end as ID, 
		case when an.individual_author is not null then 
				case when JSON_VALID(an.individual_author) = 1 then an.individual_author else '[{"text": "JSON Autor Inválido"}]' end
			when so.individual_author_monographic is not null then 
				case when JSON_VALID(so.individual_author_monographic) = 1 then so.individual_author_monographic else '[{"text": "JSON Autor Mono Inválido"}]' end
			when so.individual_author_collection is not null then 
				case when JSON_VALID(so.individual_author_collection) = 1 then so.individual_author_collection else '[{"text": "JSON Autor Coll Inválido"}]' end
		end as AU, 
		case when an.individual_author is not null then 
				case when JSON_VALID(an.individual_author) = 1 then JSON_LENGTH(an.individual_author) else 1 end
			when so.individual_author_monographic is not null then 
				case when JSON_VALID(so.individual_author_monographic) = 1 then JSON_LENGTH(so.individual_author_monographic) else 1 end
			when so.individual_author_collection is not null then 
				case when JSON_VALID(so.individual_author_collection) = 1 then JSON_LENGTH(so.individual_author_collection) else 1 end
		end as QTDE_AUTOR 
from	biblioref_reference as rf left join biblioref_referenceanalytic as an on 
		rf.id = an.reference_ptr_id left join biblioref_referencealternateid as al on 
		rf.id = al.reference_id left join biblioref_referencesource as so on 
		an.source_id = so.reference_ptr_id LIMIT ${lim_ini}, ${lim_fim};</sql>
