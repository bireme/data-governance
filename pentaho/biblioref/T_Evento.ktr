    <connection>FI-Admin</connection>
    <sql>select 	'lilacs' as FI, 
		case when rf.id = an.reference_ptr_id and rf.id = al.reference_id then  
				concat('biblio-',convert(rf.id, char))  
			when an.source_id = so.reference_ptr_id then 
				concat('lil-',convert(an.reference_ptr_id, char))
			else rf.id 
		end as ID, 
		com.conference_country_id as CN_CO, com.conference_city as CN_CY, 
		com.conference_normalized_date as CN_DA, com.conference_date as CN_DT, 
		REPLACE(com.conference_sponsoring_institution, CHAR(10), ';') as CN_IN, 
		REPLACE(com.conference_name, CHAR(10), ';') as CN_NA, 
		REPLACE(com.project_sponsoring_institution, CHAR(10), ';') as PR_IN, 
		com.project_name as PR_NA,
		CURRENT_DATE as dt_in, CURRENT_DATE as dt_up 
from	biblioref_reference as rf left join biblioref_referenceanalytic as an on 
		rf.id = an.reference_ptr_id left join biblioref_referencealternateid as al on 
		rf.id = al.reference_id left join biblioref_referencesource as so on 
		an.source_id = so.reference_ptr_id left join biblioref_referencecomplement as com on 
		rf.id = com.source_id LIMIT ${lim_ini}, ${lim_fim}; </sql>
    <limit>0</limit>
    <lookup/>
